# Tenderly Admin RPC for your Virtual TestNet on Base (Admin RPC, not public RPC)
TEST_TENDERLY_ADMIN_RPC_BASE=https://virtual.base.tenderly.co/api/v1/project/<org>/<project>/<vnet-id>

# Throwaway private key used as the primary test account (will be funded in tests)
TEST_PRIVATE_KEY=0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa

# Optional: comma-separated extra test private keys for multi-account tests
# Example: 0xbb...,0xcc...,0xdd...
TEST_PRIVATE_KEYS_CSV=

# Deployed contract addresses on Base (required)
TEST_LEVERAGE_MANAGER=0x38Ba21C6Bf31dF1b1798FCEd07B4e9b07C5ec3a8
TEST_LEVERAGE_TOKEN=0xYourDeployedLeverageToken
TEST_UNDERLYING_ERC20=0xYourUnderlyingToken