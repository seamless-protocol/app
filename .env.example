# ENVIRONMENT
NODE_ENV=development

# RPC ENDPOINTS
VITE_BASE_RPC_URL=https://base-mainnet.infura.io/v3/YOUR_INFURA_KEY
VITE_ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# WALLET CONNECT
VITE_WALLETCONNECT_PROJECT_ID=your_project_id

# API ENDPOINTS
VITE_MORPHO_API_URL=https://api.morpho.org
VITE_FUUL_API_URL=https://api.fuul.xyz
VITE_COINGECKO_API_URL=https://api.coingecko.com/api/v3

# SUBGRAPH ENDPOINTS
VITE_SEAMLESS_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/seamless/seamless-v2
VITE_FUUL_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/fuul/fuul-rewards

# ANALYTICS
VITE_SENTRY_DSN=https://your-key@sentry.io/project-id
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# IPFS GATEWAYS
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud
VITE_IPFS_API_KEY=your_pinata_api_key
VITE_IPFS_API_SECRET=your_pinata_api_secret

# FEATURE FLAGS
VITE_ENABLE_LEVERAGE_TOKEN_CREATION=false
VITE_ENABLE_GOVERNANCE=false
VITE_ENABLE_STAKING=false

# CONTRACTS (Base Network)
VITE_BASE_LEVERAGE_TOKEN_FACTORY=0x0000000000000000000000000000000000000000
VITE_BASE_VAULT_REGISTRY=0x0000000000000000000000000000000000000000
VITE_BASE_SEAM_TOKEN=0x0000000000000000000000000000000000000000
VITE_BASE_ES_SEAM_TOKEN=0x0000000000000000000000000000000000000000
VITE_BASE_STK_SEAM_TOKEN=0x0000000000000000000000000000000000000000

# CONTRACTS (Ethereum Mainnet - future)
VITE_ETH_LEVERAGE_TOKEN_FACTORY=0x0000000000000000000000000000000000000000
VITE_ETH_VAULT_REGISTRY=0x0000000000000000000000000000000000000000